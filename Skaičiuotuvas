

def sudetis(x, y):
    return x + y

def atimtis(x, y):
    return x - y

def daugyba(x, y):
    return x * y

def dalyba(x, y):
    if y != 0:
        return x / y
    else:
        return "Dalyba iš nulio negalima"
    
def skaiciuotuvas():
    print("Pasirinkite norima atlikti operacija:")
    print("1. Sudėtis")
    print("2. Atimtis")
    print("3. Daugyba")
    print("4. Dalyba")

    operacija = input("Įveskite numerį operacijai: ")

    x = float(input("Įveskite x: "))
    y = float(input("Įveskite y: "))


